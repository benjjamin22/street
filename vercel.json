{
  "version": 2,
  "name": "templates",
  "builds": [{
      "src": "./index.js",
      "use": "@vercel/node"
  }, {
      "src": "./public",
      "use": "@vercel/static"
  }],

  "rewrites": [{
      "source": "/(.*)",
      "headers":{
      "Access-Control-Allow-Origin":"*"
      },
      "destination": "index.js",
      "methods":[
          "GET",
          "POST",
          "PUT",
          "DELETE",
          "PATCH",
          "OPTIONS"
        ]
  }, {
      "src": "/",
      "dest": "public"
  }, {
      "src": "/(.*)",
      "dest": "public/$1"
  }]

}